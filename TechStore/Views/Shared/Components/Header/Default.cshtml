                <div class="col-xxl-3 col-xl-3 col-lg-3 col-md-3 col-sm-12 col-12">
                    <div class="logo">
                        <a asp-controller="home" asp-action="index"><img src="~/assets/images/logo.png" alt="logo"></a>
                    </div>
                </div>
                <div class="col-xxl-6 col-xl-6 col-lg-6 col-md-5 col-sm-12 col-12">
                    <div class="header-search">
                        <div id="overlay" onclick="off()"></div>
                        <form action="#">
                            <div id="categoryInputOnSearch" class="category">
                                All Category <i class="fa-solid fa-angle-down"></i>
                                <div class="allCategories">
                                    @foreach (Category category in Model.Categories.Where(c=>c.IsMain==true))
                                    {

                                    <div class="title">
                                        @category.Name
                                    </div>
                                    <ul>
                                        @foreach (var item in category.Children)
                                        {
                                        <li>@item.Name</li>

                                        }
                                    </ul>
                                        
                                    }
                                </div>
                            </div>
                            <input id="inputSearch" onclick="on()" type="text" placeholder="Search what you looking for?">
                            <button><img src="~/assets/images/search.png" alt=""></button>
                        </form>
                        <div class="searchSuggestions">
                            <div class="row">
                                <div class="col-lg-7">
                                    <div class="suggestions">
                                        <div class="title">
                                            Search Suggestions
                                        </div>
                                        @foreach (var item in Model.Products.Take(4))
	{
                                        <div class="row align-items-center mb-3">
                                            <div class="col-lg-3">
                                                <img src="~/assets/images/product/@item.MainImage" alt="product">
                                            </div>
                                            <div class="col-lg-9">
                                                <div class="name">
                                                    <a href="">@item.Name</a>
                                                </div>
                                                <div class="price">
                                                    <div class="sale">
                                                        $@item.DiscountedPrice
                                                    </div>
                                                    <div class="regular">
                                                        $@item.Price
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
		 
	}
                                    </div>
                                </div>
                                <div class="col-lg-5">
                                    <div class="categories">
                                        <div class="title">
                                            Categories
                                        </div>
