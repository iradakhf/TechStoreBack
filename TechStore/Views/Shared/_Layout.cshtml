@inject ILayoutService  layoutservices

@{
    IDictionary<string, string> settings = await layoutservices.GetSettings();
            }
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="~/assets/css/bootstrap.css">
    <link rel="stylesheet" type="text/css" href="//cdn.jsdelivr.net/npm/slick-carousel@1.8.1/slick/slick.css" />
    <link rel="stylesheet" type="text/css" href="//cdn.jsdelivr.net/npm/slick-carousel@1.8.1/slick/slick-theme.css" />
    <link rel="stylesheet" href="~/assets/css/style.css">
    <title>Techno Store</title>
</head>
<body>
    <!-- Header Section -->
    @(await Component.InvokeAsync("Header", settings))
    @RenderBody()
    <!-- Footer Section -->
    @(await Component.InvokeAsync("Footer",settings) )
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.1/jquery.min.js"></script>
    <script type="text/javascript" src="//cdn.jsdelivr.net/npm/slick-carousel@1.8.1/slick/slick.min.js"></script>
    <script src="~/assets/js/app.js"></script>
    <script src="~/assets/js/about.js"></script>
    <script src="~/assets/js/shop.js"></script>
    <script src="~/assets/js/brand.js"></script>
    <script src="~/assets/js/singleProduct.js"></script>
    <script src="~/assets/js/category.js"></script>
    <script src="~/assets/js/cart.js"></script>
    <script src="~/assets/js/bootstrap.js"></script>

</body>
</html>