
@model Partner
@{
    ViewData["Title"] = "Update";
}

<div class="row">
    <div class="col-lg-6 h3 mb-4 text-gray-800">
        <h1>partner Update</h1>
    </div>

    <div class="col-lg-12 mx-auto">
        <form method="post" enctype="multipart/form-data">
           
            <div class="form-group">
                <label asp-for="Link"></label>
                <input asp-for="Link" class="form-control bg-light" placeholder="Enter link">
                <span asp-validation-for="Link"></span>
            </div>
            <div class="form-group">
                <label asp-for="File"></label>
                <input asp-for="File" accept="image/png" class="form-control bg-light" placeholder="Image">
                <span asp-validation-for="File"></span>
            </div>
            <button type="submit" class="btn btn-primary">Submit</button>
        </form>
    </div>
</div>
